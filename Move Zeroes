
Example 1:

Input: nums = [0,1,0,3,12]
Output: [1,3,12,0,0]
Example 2:

Input: nums = [0]
Output: [0]


void moveZeroes(vector<int>& nums) {
         int curr=0;
         int last=0;
         while(curr < nums.size()){
            if(nums[curr]!=0){
                swap(nums[curr],nums[last]);
                last++;
            }
            curr++;
         }
         
    }
